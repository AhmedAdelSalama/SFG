<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="SFG.OverallTransferFunctionController">
   <Label fx:id="pathLabel" layoutX="18.0" layoutY="17.0" prefHeight="42.0" prefWidth="243.0" text="Foraward Paths"
          textAlignment="JUSTIFY" textOverrun="CENTER_ELLIPSIS">
      <font>
         <Font name="Arial" size="32.0"/>
      </font>
   </Label>
   <Label fx:id="loopLabel" layoutX="549.0" layoutY="21.0" prefHeight="51.0" prefWidth="99.0" text="Loops">
      <font>
         <Font name="Arial" size="32.0"/>
      </font>
   </Label>
   <Label fx:id="nonTouchingLabel" layoutX="805.0" layoutY="26.0" prefHeight="42.0" prefWidth="306.0"
          text="Non Touching Loops">
      <font>
         <Font name="Arial" size="32.0"/>
      </font>
   </Label>
   <ListView fx:id="forwardPathsList" layoutX="18.0" layoutY="77.0" prefHeight="508.0" prefWidth="243.0"/>
   <ListView fx:id="loopsList" layoutX="437.0" layoutY="77.0" prefHeight="508.0" prefWidth="323.0"/>
   <ListView fx:id="nonTouchingLoopsList" layoutX="775.0" layoutY="77.0" prefHeight="508.0" prefWidth="366.0"/>
   <TextField fx:id="transferFunctionTextField" layoutX="300.0" layoutY="669.0" prefHeight="42.0" prefWidth="366.0" promptText="TF">
      <font>
         <Font name="Arial" size="22.0"/>
      </font>
   </TextField>
   <Label fx:id="transferFnLabel" layoutX="18.0" layoutY="669.0" prefHeight="51.0" prefWidth="338.0" text="Transfer Function :">
      <font>
         <Font name="Arial" size="22.0"/>
      </font>
   </Label>
   <ListView fx:id="deltasList" layoutX="270.0" layoutY="77.0" prefHeight="508.0" prefWidth="158.0"/>
   <Label fx:id="deltaILabel" layoutX="300.0" layoutY="21.0" prefHeight="42.0" prefWidth="99.0" text="Delta i">
      <font>
         <Font name="Arial" size="32.0"/>
      </font>
   </Label>
   <Label fx:id="deltaLabel" layoutX="18.0" layoutY="613.0" prefHeight="42.0" prefWidth="306.0" text="Delta :">
      <font>
         <Font name="Arial" size="22.0"/>
      </font>
   </Label>
   <TextField fx:id="deltaTextField" layoutX="300.0" layoutY="608.0" prefHeight="42.0" prefWidth="366.0" promptText="Delta">
      <font>
         <Font name="Arial" size="22.0"/>
      </font>
   </TextField>
   <Button fx:id="NewButton" layoutX="691.0" layoutY="669.0" mnemonicParsing="false" onAction="#NewGraph" prefHeight="42.0"
           prefWidth="84.0" text="New">
      <font>
         <Font name="Arial" size="18.0"/>
      </font>
   </Button>
</AnchorPane>
