<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="122.0" prefWidth="643.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="SFG.SignalFlowGraphController">
    <TextField fx:id="numOfNodes" layoutX="173.0" layoutY="45.0" onAction="#setNumOfNodes" prefHeight="32.0"
               prefWidth="297.0">
        <font>
          <Font name="Arial" size="13.0"/>
        </font>
    </TextField>
    <Label fx:id="label" layoutX="28.0" layoutY="45.0" prefHeight="32.0" prefWidth="187.0" text="Number Of Nodes :">
        <font>
          <Font name="Arial" size="13.0"/>
        </font>
    </Label>
    <Button fx:id="okButton" layoutX="524.0" layoutY="45.0" mnemonicParsing="false" onAction="#numIsSet"
            prefHeight="32.0" prefWidth="65.0" text="Ok" textAlignment="CENTER">
        <font>
          <Font name="Arial" size="13.0"/>
        </font>
    </Button>
</AnchorPane>
